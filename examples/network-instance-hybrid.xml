<config xmlns="http://tail-f.com/ns/config/1.0">
  <interfaces xmlns="http://openconfig.net/yang/interfaces">
    <interface>
      <name>g0/0/0</name>
      <config>
        <name>g0/0/0</name>
        <type xmlns:oc-ift="http://openconfig.net/yang/openconfig-if-types">oc-ift:IF_ETHERNET</type>
      </config>
      <subinterfaces>
        <subinterface>
          <index>100</index>
          <config>
            <index>100</index>
          </config>
          <ipv6 xmlns="http://openconfig.net/yang/interfaces/ip">
            <addresses>
              <address>
                <ip>2001:DB8::12</ip>
                <config>
                  <ip>2001:DB8::12</ip>
                  <prefix-length>127</prefix-length>
                </config>
              </address>
            </addresses>
          </ipv6>
          <vlan xmlns="http://openconfig.net/yang/vlan">
            <config>
              <vlan-id>100</vlan-id>
            </config>
          </vlan>
        </subinterface>
      </subinterfaces>
      <ethernet xmlns="http://openconfig.net/yang/interfaces/ethernet">
        <switched-vlan xmlns="http://openconfig.net/yang/vlan">
          <config>
            <interface-mode>TRUNK</interface-mode>
            <trunk-vlans>1026..1030</trunk-vlans>
            <trunk-vlans>1024</trunk-vlans>
            <trunk-vlans>1025</trunk-vlans>
          </config>
        </switched-vlan>
      </ethernet>
    </interface>
  </interfaces>
  <network-instances xmlns="http://openconfig.net/yang/network-instance">
    <network-instance>
      <name>GLOBAL</name>
      <config>
        <name>GLOBAL</name>
        <type xmlns:oc-ni-types="http://openconfig.net/yang/network-instance-types">oc-ni-types:DEFAULT_INSTANCE</type>
      </config>
      <interfaces>
        <interface>
          <id>g0/0/0</id>
          <config>
            <id>g0/0/0</id>
            <interface>g0/0/0</interface>
          </config>
        </interface>
      </interfaces>
      <vlans>
        <vlan>
          <vlan-id>1024</vlan-id>
          <config>
            <vlan-id>1024</vlan-id>
            <name>vlan1024</name>
          </config>
        </vlan>
        <vlan>
          <vlan-id>1025</vlan-id>
          <config>
            <vlan-id>1025</vlan-id>
            <name>vlan1025</name>
          </config>
        </vlan>
        <vlan>
          <vlan-id>1026</vlan-id>
          <config>
            <vlan-id>1026</vlan-id>
            <name>vlan1026</name>
          </config>
        </vlan>
        <vlan>
          <vlan-id>1027</vlan-id>
          <config>
            <vlan-id>1027</vlan-id>
            <name>vlan1027</name>
          </config>
        </vlan>
        <vlan>
          <vlan-id>1028</vlan-id>
          <config>
            <vlan-id>1028</vlan-id>
            <name>vlan1028</name>
          </config>
        </vlan>
        <vlan>
          <vlan-id>1029</vlan-id>
          <config>
            <vlan-id>1029</vlan-id>
            <name>vlan1029</name>
          </config>
        </vlan>
      </vlans>
    </network-instance>
    <network-instance>
      <name>SOME_VPN</name>
      <config>
        <name>SOME_VPN</name>
        <type xmlns:oc-ni-types="http://openconfig.net/yang/network-instance-types">oc-ni-types:L3VRF</type>
      </config>
      <interfaces>
        <interface>
          <id>SALT-LAKE-CITY-OFFICE</id>
          <config>
            <id>SALT-LAKE-CITY-OFFICE</id>
            <interface>g0/0/0</interface>
            <subinterface>100</subinterface>
          </config>
        </interface>
      </interfaces>
    </network-instance>
  </network-instances>
</config>
